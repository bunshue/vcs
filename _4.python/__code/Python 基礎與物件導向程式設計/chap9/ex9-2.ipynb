    "import threading\n",
    "\n",
    "def run(which):\n",
    "    print(which, '啟動了！')\n",
    "    for i in range(100):\n",
    "        print(which, i)\n",
    "    print(which, '完成了！')\n",
    "\n",
    "thr1 = threading.Thread(target=run, args=('1'))\n",
    "thr2 = threading.Thread(target=run, args=('2'))\n",
    "thr1.start()\n",
    "thr2.start()"
   ]
