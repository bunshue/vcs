    "def interpDiv(a, b):\n",
    "    r = a % b;\n",
    "    if r==0:\n",
    "        return b;\n",
    "    # 遞迴!\n",
    "    return interpDiv(b, r);\n",
    "\n",
    "x, y = 5929, 8281\n",
    "print('GCD =', interpDiv(y, x))"
