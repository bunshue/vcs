   "source": [
    "def fibonacci(n):\n",
    "    # 遞迴中止條件！！\n",
    "    if n==0 or n==1:\n",
    "        return 1\n",
    "    return fibonacci(n-1)+fibonacci(n-2)\n",
    "\n",
    "# 遞迴中止條件！！\n",
    "for i in range(10):\n",
    "    n = i+1\n",
    "    print(n, fibonacci(n))"
   ]
