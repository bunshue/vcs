   "source": [
    "def factorial(n):\n",
    "    # 遞迴中止條件！！\n",
    "    if n==1:\n",
    "        return 1\n",
    "    return n*factorial(n-1)\n",
    "\n",
    "for i in range(10):\n",
    "    n = i+1\n",
    "    print(n, factorial(n))"
   ]
