    "# 定義自訂的函數 \n",
    "def adjust_scores(scores):\n",
    "    index = 0\n",
    "    for score in scores:\n",
    "        scores[index] = score+20 if score<80 else 100\n",
    "        index += 1\n",
    "\n",
    "scores = [10, 40, 96, 77]\n",
    "adjust_scores(scores)\n",
    "print(scores)"
   ]
