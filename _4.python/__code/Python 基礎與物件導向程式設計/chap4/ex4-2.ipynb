    "# 串列資料\n",
    "list1 = [3, 4, 5, 6, 7, 8]\n",
    "list1[0] = 30\n",
    "list1[-1] = list1[-1]*10\n",
    "print(list1)                # [30, 4, 5, 6, 7, 80]\n",
    "\n",
    "# del敘述用來刪除物件\n",
    "del list1[-1]\n",
    "print(list1)                # [30, 4, 5, 6, 7]\n",
    "del list1[2]\n",
    "print(list1)                # [30, 4, 6, 7]"
