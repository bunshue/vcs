void preload_picture_to_memory(void)
{
	int status;
	//volatile uint32_t g_ms_tick_tmp1;
	//volatile uint32_t g_ms_tick_tmp2;
	coord_t width = LAYER1_WIDTH;
	coord_t height = LAYER1_HEIGHT;

