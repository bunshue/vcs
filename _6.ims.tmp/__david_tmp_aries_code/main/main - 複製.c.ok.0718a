	}

	uint32_t addr0;
	uint32_t addr1;
	addr0 = (uint32_t)addr;
	addr1 = (uint32_t)gdispGetFrameBufferAddr(frame);

	uint32_t addr2;

	for(i = 0; i < H; i++)
	{
		addr2 = (u32)(LAYER0_WIDTH * 4 * (LAYER1_START_Y + i) + addr1 + 4 * LAYER1_START_X);
